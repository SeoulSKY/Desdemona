{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [true, "double", "avoid-escape"],
    "indent": [true, "spaces", 2],
    "object-literal-sort-keys": false,
    "variable-name": [true, "ban-keywords", "check-format"]
  },
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts", "public/lib/**/*.ts"]
  }
}
